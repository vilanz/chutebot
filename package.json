    "common-tags": "^1.8.0",
    "country-emoji": "^1.5.6",
    "date-fns": "^2.22.1",
    "discord.js": "^13.0.0-dev.5af2ef5fbc7ad11281f38384c360ae79efe63b39",
    "dotenv": "^10.0.0",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "table": "^6.7.1",
    "ts-node": "^10.0.0",
    "typeorm": "0.2.36",
    "typescript": "^4.3.4",
    "user-agents": "^1.0.695",
