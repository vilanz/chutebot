{
  "name": "chutebot",
  "author": "vilanz",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && pm2 start pm2.config.js",
    "start:prod": "yarn start --env prod",
    "rebuild": "yarn stop && yarn start",
    "rebuild:prod": "yarn restart --env prod",
    "stop": "pm2 stop bot || echo Already stopped",
    "kill": "pm2 delete bot",
    "logs": "pm2 logs bot",
    "build": "rm -rf build && tsc && cp .env build/.env",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@types/better-sqlite3": "^5.4.3",
    "@vilanz/tooling": "vilanz/tooling#main",
    "axios": "^0.21.1",
